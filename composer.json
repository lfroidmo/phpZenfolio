{
    "name": "lildude/phpzenfolio",
    "type": "library",
    "description": "A simple object orientated wrapper for the Zenfolio API",
    "homepage": "http://phpzenfolio.com",
    "keywords": ["zenfolio", "api"],
    "license": "MIT",
    "authors": [
        {
            "name": "Colin Seymour",
            "homepage": "http://lildude.co.uk"
        },
        {
            "name": "phpZenfolio Contributors",
            "homepage": "https://github.com/lildude/phpZenfolio/graphs/contributors"
        }
    ],
    "require": {
        "php": ">=5.6.0",
        "guzzlehttp/guzzle": "~6.2"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.0",
        "friendsofphp/php-cs-fixer": "~1.11",
        "satooshi/php-coveralls": "dev-master"
    },
    "autoload": {
        "psr-4": { "phpZenfolio\\": "lib/phpZenfolio/" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0.x-dev"
        }
    }
}
